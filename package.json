{
  "name": "remark-preset-prettier",
  "version": "2.0.1",
  "type": "module",
  "description": "Turns off all rules that are unnecessary or might conflict with Prettier.",
  "repository": "https://@github.com/un-ts/remark-preset-prettier.git",
  "author": "JounQin <admin@1stg.me> (https://www.1stG.me)",
  "license": "MIT",
  "packageManager": "yarn@4.8.1",
  "engines": {
    "node": ">=14.8"
  },
  "main": "lib/index.js",
  "exports": {
    "types": "./lib/index.d.ts",
    "default": "./lib/index.js"
  },
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.lib.json",
    "format": "prettier --write .",
    "lint": "yarn build && run-p 'lint:*'",
    "lint:es": "eslint . --cache",
    "lint:ts": "tsc --noEmit",
    "prepare": "simple-git-hooks && yarn-berry-deduplicate || exit 0",
    "release": "yarn build && clean-pkg-json && changeset publish",
    "typecov": "type-coverage"
  },
  "peerDependencies": {
    "prettier": ">=1.0.0"
  },
  "devDependencies": {
    "@1stg/common-config": "^11.1.2",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@commitlint/cli": "^19.8.0",
    "@types/node": "^22.14.0",
    "clean-pkg-json": "^1.2.1",
    "eslint": "^9.23.0",
    "lint-staged": "^15.5.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "simple-git-hooks": "^2.12.1",
    "size-limit": "^11.2.0",
    "size-limit-preset-node-lib": "^0.3.0",
    "type-coverage": "^2.29.7",
    "typescript": "^5.8.2",
    "yarn-berry-deduplicate": "^6.1.1"
  },
  "resolutions": {
    "es5-ext": "npm:@unes/es5-ext@latest",
    "prettier": "^3.5.3",
    "remark-preset-prettier": "link:."
  },
  "typeCoverage": {
    "atLeast": 100,
    "cache": true,
    "detail": true,
    "ignoreAsAssertion": true,
    "ignoreCatch": true,
    "ignoreFiles": [
      "lib"
    ],
    "showRelativePath": true,
    "strict": true,
    "update": true
  }
}
